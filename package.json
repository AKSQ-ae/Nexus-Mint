{
  "name": "nexus-mint",
  "private": true,
  "version": "1.0.0",
  "description": "Enterprise-grade real estate tokenization platform with AI-powered investment assistant",
  "type": "module",
  "engines": {
    "node": "18.x",
    "npm": ">=9.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "lighthouse": "lhci autorun",
    "analyze:bundle": "npm run build && source-map-explorer 'dist/assets/index-*.js'",
    "analyze:css": "npm run build && source-map-explorer 'dist/assets/index-*.css'",
    "contracts:compile": "cd contracts && npx hardhat compile",
    "contracts:test": "cd contracts && npx hardhat test",
    "contracts:deploy": "cd contracts && npx hardhat run scripts/deploy.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf dist node_modules .turbo",
    "install:clean": "rm -rf node_modules pnpm-lock.yaml && pnpm install"
  },
  "dependencies": {
