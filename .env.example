# Nexus Mint Environment Variables
# Copy this file to .env.local and fill in your values

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# Blockchain Configuration
VITE_POLYGON_RPC_URL=https://polygon-mainnet.g.alchemy.com/v2/your-api-key
VITE_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000

# Stripe Configuration (optional)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your-key

# 11Labs AI Configuration (optional)
VITE_11LABS_API_KEY=your-api-key

# Sentry Error Tracking (optional)
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Application Configuration
VITE_APP_NAME="Nexus Mint"
VITE_APP_URL=https://app.nexus-mint.com

# Feature Flags
VITE_ENABLE_AI_ASSISTANT=true
VITE_ENABLE_MOBILE_APP=true

# AWS Configuration (for deployment)
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=your-account-id